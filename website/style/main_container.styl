mainHeight = 90vh
infoWidth = 400px
flex()
    display flex
    align-items center
#mainContainer
    height mainHeight
    #gridPage
        display none
        #warringTip
            display none
            z-index 100
            position fixed
            top 10vh
            right 0
            height 200px
            width 400px
            padding-left 20px
            background-color #fff
            color #000
            font-size 16px
            h1
                color #F56C6C
                border-bottom 1px solid #777
            div
                margin-top 10px
            .warnningInfo
                font-size 18px
                font-style italic
                text-decoration underline
                
        #traContainer
            position  fixed
            z-index  -1
            top 10vh
            height 90vh
            width 100vw
    .page
        height mainHeight
        #map
            height mainHeight;
            padding 0
            margin 0
        #shipInfo
            height 300px
            width infoWidth
            position fixed
            z-index 100
            background-color #F0F8FF
            border 1px solid #000
            display none
            .shipInfoLine
                flex()
                height 60px
                width 100%
                border-top 1px solid #000
                .shipInfoItem
                    width 50%
                    padding-left 10px
                    span
                        font-size 18px
                        color #555
                        font-style italic